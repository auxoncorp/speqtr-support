            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "group_by",
            "in": "query",
            "description": "Grouping attr key",
            "required": false,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "nullable": true
            },
            "explode": true
          }
        ],
        "responses": {
          "200": {
            "description": "Retrieve the graph successfully",
            "content": {
              "application/json": {
                "schema": {
